{
  "name": "Builds Manager",
  "version": "0.0.1",
  "namespace": "Kenedia.Modules.BuildsManager",
  "package": "Builds.dll",
  "manifest_version": 1,

  "description": "A Blish approach to enhance your gameplay experience with the 'Legendary Armory' and 'Build Templates'. A must have for everyone with a lot of builds!",
  "dependencies": {
    "bh.blishhud": ">=0.11.7"
  },
  "url": "https://github.com/blish-hud/Blish-HUD",
  "contributors": [
    {
      "url": "https://github.com/KenediaDev",
      "name": "Kenedia"
    }
  ],

  "directories": [ "builds-manager", "builds-manager/api", "builds-manager/api/traits", "builds-manager/api/traits/icons", "builds-manager/api/facts", "builds-manager/api/skills", "builds-manager/api/skills/icons", "builds-manager/api/specs", "builds-manager/api/specs/backgrounds", "builds-manager/api/specs/icons", "builds-manager/api/stats", "builds-manager/api/armory", "builds-manager/api/armory/icons" ],
  "enabled_without_gw2": false,

  "api_permissions": {
    "account": {
      "optional": true,
      "details": "General Account Informations to store files in a specific account folder."
    },
    "characters": {
      "optional": true,
      "details": "Needed to get your characters."
    },
    "unlocks": {
      "optional": true,
      "details": "Needed to get your armory items."
    }
  }
}